    <string name="recipe_25_ing_2">ملعقتان كبيرتان طحينة</string>
    <string name="recipe_25_ing_3">عصير ليمونة واحدة</string>
    <string name="recipe_25_ing_4">فصّ ثوم واحد</string>
    <string name="recipe_25_ing_5">أصابع جزر وكرفس وخيار</string>
    <string name="recipe_25_ing_6">بابريكا، زيت زيتون للرش</string>
