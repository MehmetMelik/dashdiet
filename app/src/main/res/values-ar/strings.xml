    <string name="recipe_27_ing_1">حبة تفاح متوسطة</string>
    <string name="recipe_27_ing_2">ملعقتان كبيرتان زبدة لوز طبيعية</string>
    <string name="recipe_27_ing_3">رشة قرفة</string>
